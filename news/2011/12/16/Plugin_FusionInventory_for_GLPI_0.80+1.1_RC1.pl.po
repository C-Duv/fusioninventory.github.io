#. type: Plain text
msgid ""
"For people have installed BETA version, apply these SQL queries to force "
"plugins to update to this RC1 :"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"<blockquote>\n"
"UPDATE `glpi_plugin_fusioninventory_configs` SET `value` = '2.4.0' WHERE "
"`glpi_plugin_fusioninventory_configs`.`type` = 'version';\n"
"UPDATE `glpi_plugins` SET `version` = '2.4.0' WHERE "
"`directory`='fusioninventory' OR `directory`='fusinvinventory' OR "
"`directory`='fusinvsnmp';\n"
"</blockquote>\n"
msgstr ""
