msgstr ""

#. type: Plain text
msgid "- __\\--ca-cert-file__=_FILE_"
msgstr ""

#. type: Plain text
msgid "Path to the CA certificates file."
msgstr ""

#. type: Plain text
msgid "- __\\--no-ssl-check__"
msgstr ""

#. type: Plain text
msgid "Do not check server SSL certificates."
msgstr ""

#. type: Plain text
