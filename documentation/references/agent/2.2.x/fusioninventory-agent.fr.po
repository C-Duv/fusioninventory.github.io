#. type: Title #
#, no-wrap
msgid "NAME"
msgstr "NOM"

#. type: Plain text
msgid ""
"fusioninventory-agent - FusionInventory agent For Linux/UNIX, Windows and "
"MacOSX"
msgstr ""
"fusioninventory-agent - Agent FusionInventory pour Linux/UNIX, Windows et "
"MacOSX"

#. type: Title #
#, no-wrap
msgid "SYNOPSIS"
msgstr "SYNOPSIS"

#. type: Plain text
msgid ""
"__fusioninventory-agent__ \\[options\\] \\[--server server|--local "
"directory|--sdout\\]"
msgstr ""
"__fusioninventory-agent__ \\[options\\] \\[--server serveur|--local "
"dossier|--sdout\\]"

#. type: Plain text
#, no-wrap
msgid ""
"    Target definition options\n"
"      -s --server=URI     send tasks result to a server\n"
"      -l --local=DIR      write tasks results in a directory\n"
"      --stdout            write tasks result on STDOUT\n"
msgstr ""
"    Options pour definir la destination des résultats\n"
"      -s --server=URI     Envoyer les résultats à un serveur\n"
"      -l --local=DIR      Écrire les résultats dans un dossier\n"
"      --stdout            Envoyer les résultats sur la sortie STDOUT\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Target scheduling options\n"
"      --delaytime=DURATION        maximum initial delay before first target, in seconds (3600)\n"
"      -w --wait=DURATION          maximum delay between each target, in seconds\n"
"      --lazy                      do not contact the target before next scheduled time\n"
msgstr ""
"    Options de planification de l'agent\n"
"      --delaytime=DURÉE        Temps initial maximal avant le 1er lancement, en seconds (3600)\n"
"      -w --wait=DURÉE          Délai maximal entre chaque lancement, en seconds\n"
"      --lazy                     Ne pas lancer l'agent avant la prochaine heure planifiée\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Task selection options\n"
"      --list-tasks        list available tasks and exit\n"
"      --no-task=TASK      do not run given task\n"
msgstr ""
"    Options de selection des tâches\n"
"      --list-tasks        Liste les tâches valides et sortie\n"
"      --no-task=TASK      ne pas exécuter la tâche donnée\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Inventory task specific options\n"
"      --no-category=CATEGORY do not list given category items\n"
"      --scan-homedirs     allow to scan use home directories (false)\n"
"      --html              save the inventory as HTML (false)\n"
"      -f --force          always send data to server (false)\n"
"      -t --tag=TAG        mark the machine with given tag\n"
"      --backend-collect-timeout   timeout for inventory modules execution (30)\n"
"      --additional-content=FILE   additional inventory content\n"
msgstr ""
"    Options spécifiques de l'inventaire\n"
"      --no-category=CATÉGORIE  ne dresse pas la liste des éléments de catégorie donnée\n"
"      --scan-homedirs          Active l'analyse des dossiers personnels (false)\n"
"      --html                   Enregistre l'inventaire en HTML (false)\n"
"      -f --force               Toujours envoyer les données au serveur (false)\n"
"      -t --tag=TAG             Assigne à la machine ce TAG\n"
"      --backend-collect-timeout   délai d'attente pour l'exécution des modules d'inventaire (30)\n"
"      --additional-content=FILE   contenu inventaire supplémentaire\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Package deployment task specific options\n"
"      --no-p2p            do not use peer to peer to download files (false)\n"
msgstr ""
"    Option spécifique au tâche de déploiement de paquet\n"
"      --no-p2p            Ne pas utiliser le téléchargement de fichier par peer to peer (P2P) (false)\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Network options:\n"
"      -P --proxy=PROXY    proxy address\n"
"      -u --user=USER      user name for server authentication\n"
"      -p --password=PWD   password for server authentication\n"
"      --ca-cert-dir=D     path to the CA certificates directory\n"
"      --ca-cert-file=F    path to the CA certificates file\n"
"      --no-ssl-check      do not check server SSL certificates (false)\n"
"      --timeout           connection timeout (180)\n"
msgstr ""
"    Options réseau:\n"
"      -P --proxy=PROXY    adresse du proxy\n"
"      -u --user=USER      Nom pour authentification sur le serveur\n"
"      -p --password=PWD   Mot de passe pour authentification sur le serveur\n"
"      --ca-cert-dir=D     Chemin du dossier du certificat CA-cert\n"
"      --ca-cert-file=F    Chemin du fichier CA-cert\n"
"      --no-ssl-check      Ne pas vérifier le certificat SSL du serveur(false)\n"
"      --timeout           Délai maximum de connexion (180)\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Web interface options\n"
"      --no-httpd                 disable embedded web server (false)\n"
"      --httpd-ip=IP              network interface to listen to\n"
"      --httpd-port=PORT          network port to listen to (62354)\n"
"      --httpd-trust=RANGE        trust requests from range without authentication token (false)\n"
msgstr ""
"    Options de l'interface web de l'agent\n"
"      --no-httpd                 Désactive le serveur web intégré (false)\n"
"      --httpd-ip=IP              Adresse IP utilisée par l'interface web\n"
"      --httpd-port=PORT          Port réseau utilisé par l'interface web(62354)\n"
"      --httpd-trust=RANGE        Accepter les requêtes de la plage désignée sans authentification \n"

#. type: Plain text
#, no-wrap
msgid ""
"    Logging options\n"
"      --logger=BACKEND            logger backend [Stderr|File|Syslog] (Stderr)\n"
"      --logfile=FILE              log file\n"
"      --logfile-maxsize=X         maximum size of the log file in MB (0)\n"
"      --logfacility=FACILITY      syslog facility (LOG_USER)\n"
"      --color                     use color in the console (false)\n"
msgstr ""
"    Options du log\n"
"      --logger=BACKEND            Type de log [Stderr=>sortie erreur|File=>fichier|Syslog=>gestionnaire log] (Stderr)\n"
"      --logfile=FILE              Nom du fichier de log\n"
"      --logfile-maxsize=X         Taille maxi du fichier de log en MB (0)\n"
"      --logfacility=FACILITY      Option facility pour syslog (LOG_USER)\n"
"      --color                     Utiliser la couleur dans la console (false)\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Configuration options\n"
"      --config=BACKEND            configuration backend [file|registry|none]\n"
"      --conf-file=FILE            configuration file\n"
msgstr ""
"    Options de configuration\n"
"      --config=BACKEND            Type de configuration [file=>fichier|registry=>base de registre|aucune]\n"
"      --conf-file=FILE            Fichier de configuration\n"

#. type: Plain text
#, no-wrap
msgid ""
"    Execution mode options\n"
"      -d --daemon                 run the agent as a daemon (false)\n"
"      --no-fork                   don't fork in background (false)\n"
"      --debug                     debug mode (false)\n"
"      --version                   print the version and exit\n"
msgstr ""
"    Options sur le mode d'exécution\n"
"      -d --daemon                 Lance l'agent en mode démon(false)\n"
"      --no-fork                   Ne pas dupliquer en arrière plan(false)\n"
"      --debug                     Mode debug (false)\n"
"      --version                   Affiche juste la version\n"

