msgstr ""

#. type: Title ##
#, no-wrap
msgid "Install the additional packages"
msgstr ""

#. type: Plain text
msgid "Network inventory tasks:"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    apt-get install fusioninventory-agent-task-network\n"
msgstr ""

#. type: Plain text
msgid "ESX inventory task:"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    apt-get install fusioninventory-agent-task-esx\n"
msgstr ""

#. type: Plain text
msgid "Deploy task:"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    apt-get install fusioninventory-agent-task-deploy\n"
msgstr ""
