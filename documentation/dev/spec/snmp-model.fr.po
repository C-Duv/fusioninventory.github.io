#. type: Title ###
#, no-wrap
msgid "Proposal 2"
msgstr ""

#. type: Plain text
msgid "Drop useless informations from the model. Here is an alternate DTD:"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT model (type,key,portCounter,vlanInfo,devices,properties)>\n"
"    <!ELEMENT type (NETWORKING|PRINTER)>\n"
"    <!ELEMENT key (#PCDATA)>\n"
"    <!ELEMENT portCounter (#PCDATA)>\n"
"    <!ELEMENT vlanInfo (#PCDATA)>\n"
"    <!ELEMENT devices (sysdescr*)>\n"
"    <!ELEMENT sysdescr (#PCDATA)>\n"
"    <!ELEMENT properties (property*)>\n"
"    <!ELEMENT property (name,oid,activation)*>\n"
"    <!ELEMENT name (#PCDATA)>\n"
"    <!ELEMENT oid (#PCDATA)>\n"
"    <!ELEMENT activation (0|1)>\n"
msgstr ""

#. type: Plain text
msgid "And a sample instance:"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <model>\n"
"        <key>Printer0547</key>\n"
"        <type>PRINTER</type>\n"
"        <portCounter></portCounter>\n"
"        <vlanInfo>0</vlanInfo>\n"
"        ...\n"
"        <properties>\n"
"            <property>\n"
"\t\t<name>comments</name>\n"
"\t\t<oid>.1.3.6.1.2.1.1.1.0</oid>\n"
"\t\t<activation>1</activation>\n"
"            </property>\n"
"        ...\n"
"            <property>\n"
"\t\t<name>ifaddr</name>\n"
"\t\t<oid>.1.3.6.1.2.1.4.20.1.2</oid>\n"
"\t\t<activation>1</activation>\n"
"            </property>\n"
"        </properties>\n"
"    </model>\n"
msgstr ""

#. type: Title ###
#, no-wrap
msgid "Proposal 3"
msgstr ""

#. type: Plain text
msgid ""
"Drop default values from models, hardcode them in the agent, and use models "
"to push exceptions only."
msgstr ""
