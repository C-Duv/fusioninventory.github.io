#. type: Title #
#, no-wrap
msgid "Network inventory protocol"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "Server to agent message"
msgstr ""

#. type: Title ###
#:  
#, no-wrap
msgid "Specification"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT REPLY (PROCESSNUMBER, OPTION)>\n"
"    <!ELEMENT PROCESSNUMBER (#PCDATA)>\n"
"    <!ELEMENT OPTION (NAME, PARAM, DEVICE+, AUTHENTICATION+)>\n"
"    <!ELEMENT NAME (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT PARAM EMPTY>\n"
"    <!ATTLIST PARAM THREADS_QUERY CDATA \"1\">\n"
"    <!ATTLIST PARAM PID CDATA #REQUIRED>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT DEVICE EMPTY>\n"
"    <!ATTLIST DEVICE TYPE "
"(COMPUTER|PRINTER|NETWORKING|STORAGE|POWER|PHONE|VIDEO) #REQUIRED>\n"
"    <!ATTLIST DEVICE ID CDATA #REQUIRED>\n"
"    <!ATTLIST DEVICE IP CDATA #REQUIRED>\n"
"    <!ATTLIST DEVICE IP AUTHSNMP_ID #REQUIRED>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT AUTHENTICATION EMPTY>\n"
"    <!ATTLIST AUTHENTICATION ID CDATA #REQUIRED>\n"
"    <!ATTLIST AUTHENTICATION VERSION CDATA #REQUIRED>\n"
"    <!ATTLIST AUTHENTICATION COMMUNITY CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION USERNAME CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION AUTHPROTOCOL CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION AUTHPASSPHRASE CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION PRIVHPROTOCOL CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION PRIVPASSPHRASE CDATA #IMPLIED>\n"
msgstr ""

#. type: Title ###
#, no-wrap
msgid "Example"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <?xml version=\"1.0\" encoding=\"UTF-8\"?>\n"
"    <REPLY>\n"
"      <PROCESSNUMBER>1280265498/024</PROCESSNUMBER>\n"
"      <OPTION>\n"
"        <NAME>SNMPQUERY</NAME>\n"
"        <PARAM THREADS_QUERY=\"4\" PID=\"1280265498/024\"/>\n"
"        <DEVICE TYPE=\"PRINTER\" ID=\"72\" IP=\"192.168.0.151\" "
"AUTHSNMP_ID=\"1\"/>\n"
"        <AUTHENTICATION ID=\"1\" COMMUNITY=\"public\" VERSION=\"1\" "
"USERNAME=\"\" AUTHPROTOCOL=\"\" AUTHPASSPHRASE=\"\" PRIVPROTOCOL=\"\" "
"PRIVPASSPHRASE=\"\"/>\n"
"      </OPTION>\n"
"    </REPLY>\n"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "Agent to server message"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT REQUEST (CONTENT+ , DEVICEID, QUERY)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT CONTENT (DEVICE*, MODULEVERSION, PROCESSNUMBER)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The devices list -->\n"
"    <!ELEMENT DEVICE (INFO , PORTS)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The device info -->\n"
"    <!ELEMENT INFO (COMMENTS, CPU, FIRMWARE, ID, IPS, LOCATION, MAC, MEMORY, "
"MODEL, NAME, RAM, SERIAL, TYPE, UPTIME)>\n"
"    <!-- it's the sysdescr (string) -->\n"
"    <!ELEMENT COMMENTS (#PCDATA)>\n"
"    <!-- it's the cpu load in %, so from 0 to 100 (integer) -->\n"
"    <!ELEMENT CPU (#PCDATA)>\n"
"    <!-- it's the firmware (string) -->\n"
"    <!ELEMENT FIRMWARE (#PCDATA)>\n"
"    <!-- it's the id of the device in GLPI (integer) -->\n"
"    <!ELEMENT ID (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The IPs list -->\n"
"    <!ELEMENT IPS (IP+)>\n"
"    <!-- it's the IP address, can be IP v4 and v6 (string) -->\n"
"    <!ELEMENT IP (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- it's the location (string) -->\n"
"    <!ELEMENT LOCATION (#PCDATA)>\n"
"    <!-- it's the mac address like 00:5e:03:a1:00:11 with letter always in "
"lowercase (string) -->\n"
"    <!ELEMENT MAC (#PCDATA)>\n"
"    <!-- it's the storage in Mio (integer) -->\n"
"    <!ELEMENT MEMORY (#PCDATA)>\n"
"    <!-- it's the device model (string) -->\n"
"    <!ELEMENT MODEL (#PCDATA)>\n"
"    <!-- it's the device name (string) -->\n"
"    <!ELEMENT NAME (#PCDATA)>\n"
"    <!-- it's the volatile memory in Mio (integer) -->\n"
"    <!ELEMENT RAM (#PCDATA)>\n"
"    <!-- it's the device serial number (string) -->\n"
"    <!ELEMENT SERIAL (#PCDATA)>\n"
"    <!-- it's the type of device NETWORKING, PRINTER, COMPUTER or PHONE "
"(constant) -->\n"
"    <!ELEMENT TYPE (#PCDATA)>\n"
"    <!-- it's the device uptime in format 8 days, 01:48:57.95 (string) -->\n"
"    <!ELEMENT UPTIME (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The device ports list -->\n"
"    <!ELEMENT PORTS (PORT*)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- One port -->\n"
"    <!ELEMENT PORT (CONNECTIONS?, AGGREGATE?, IFDESCR, IFINERRORS, "
"IFINOCTETS, IFINTERNALSTATUS, IFLASTCHANGE, IFMTU, IFNAME, IFNUMBER, "
"IFOUTERRORS, IFOUTOCTETS, IFSPEED, IFSTATUS, IFTYPE, MAC, TRUNK, VLANS?)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The port connections list -->\n"
"    <!ELEMENT CONNECTIONS (CDP?, CONNECTION*)>\n"
"    <!-- This port has a LLDP/CDP connection? (bool) -->\n"
"    <!ELEMENT CDP (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- One connection info of the port -->\n"
"    <!ELEMENT CONNECTION (MAC+ | (IFNUMBER?, SYSMAC?, IFDESCR?, IP?, "
"SYSDESCR?, SYSNAME?))>\n"
"    <!-- it's the mac address of the remote device, like 00:5e:03:a1:00:11 "
"with letter always in lowercase (string) -->\n"
"    <!ELEMENT MAC (#PCDATA)>\n"
"    <!-- it's the port number of remote device (integer) -->\n"
"    <!ELEMENT IFNUMBER (#PCDATA)>\n"
"    <!-- it's the mac address of the remote device, like 00:5e:03:a1:00:11 "
"with letter always in lowercase. Differ of MAC because get with LLDP oids "
"(string) -->\n"
"    <!ELEMENT SYSMAC (#PCDATA)>\n"
"    <!-- it's the port description of remote device (string) -->\n"
"    <!ELEMENT IFDESCR (#PCDATA)>\n"
"    <!-- it's the IP address of renote device, can be IP v4 and v6 (string) "
"-->\n"
"    <!ELEMENT IP (#PCDATA)>\n"
"    <!-- it's the sysdescr of remote device (string) -->\n"
"    <!ELEMENT SYSDESCR (#PCDATA)>\n"
"    <!-- it's the name of remote device (string) -->\n"
"    <!ELEMENT SYSNAME (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The aggregate port list, so the virtual port base on many physical "
"ports -->\n"
"    <!ELEMENT AGGREGATE (PORT+)>\n"
"    <!-- it's the ifnumber of physical port (integer) -->\n"
"    <!ELEMENT PORT (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- it's the description of port (string) -->\n"
"    <!ELEMENT IFDESCR (#PCDATA)>\n"
"    <!-- it's the number of errors in input of port (integer) -->\n"
"    <!ELEMENT IFINERRORS (#PCDATA)>\n"
"    <!-- it's the number of bytes in input of port (integer) -->\n"
"    <!ELEMENT IFINOCTETS (#PCDATA)>\n"
"    <!-- it's the internal status of port (integer, is 1, 2 or 3) -->\n"
"    <!ELEMENT IFINTERNALSTATUS (#PCDATA)>\n"
"    <!-- it's the time since last change of port in format 8 days, "
"01:48:57.95 (string) -->\n"
"    <!ELEMENT IFLASTCHANGE (#PCDATA)>\n"
"    <!-- it's the MTU of port (integer) -->\n"
"    <!ELEMENT IFMTU (#PCDATA)>\n"
"    <!-- it's the name of port (string) -->\n"
"    <!ELEMENT IFNAME (#PCDATA)>\n"
"    <!-- it's the number of port (integer) -->\n"
"    <!ELEMENT IFNUMBER (#PCDATA)>\n"
"    <!-- it's the number of errors in output of port (integer) -->\n"
"    <!ELEMENT IFOUTERRORS (#PCDATA)>\n"
"    <!-- it's the number of bytes in output of port (integer) -->\n"
"    <!ELEMENT IFOUTOCTETS (#PCDATA)>\n"
"    <!-- it's the speed in bytes of port (integer) -->\n"
"    <!ELEMENT IFSPEED (#PCDATA)>\n"
"    <!-- it's the status of port, connected or not (bool) -->\n"
"    <!ELEMENT IFSTATUS (#PCDATA)>\n"
"    <!-- it's the type of port (integer) -->\n"
"    <!ELEMENT IFTYPE (#PCDATA)>\n"
"    <!-- it's the mac address of port, like 00:5e:03:a1:00:11 with letter "
"always in lowercase (string) -->\n"
"    <!ELEMENT MAC (#PCDATA)>\n"
"    <!-- it's the trunk info of port, trunk/tagged or not (bool) -->\n"
"    <!ELEMENT TRUNK (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- The port VLAN list -->\n"
"    <!ELEMENT VLANS (VLAN+)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- One VLAN -->\n"
"    <!ELEMENT VLAN (NAME, NUMBER)>\n"
"    <!-- it's the name of VLAN (string) -->\n"
"    <!ELEMENT NAME (#PCDATA)>\n"
"    <!-- it's the number of VLAN (integer) -->\n"
"    <!ELEMENT NUMBER (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- it's the version of netinventory agent module (string) -->\n"
"    <!ELEMENT MODULEVERSION (#PCDATA)>\n"
"    <!-- it's the id of the process sent by server (integer) -->\n"
"    <!ELEMENT PROCESSNUMBER (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT DEVICEID (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT QUERY (#PCDATA)>\n"
msgstr ""
