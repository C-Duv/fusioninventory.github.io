#. type: Title #
#, no-wrap
msgid "Network discovery protocol"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "Server to agent message"
msgstr ""

#. type: Title ###
#, no-wrap
msgid "Specification"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    {\n"
"        'id': ''string'',\n"
"        'type': 'netdiscovery',\n"
"        'workers': ''integer'',\n"
"        'targets': [\n"
"            {\n"
"                'network': ''string'',\n"
"                'workers': ''integer'',\n"
"                'timeout': ''integer'',\n"
"                'credentials': [\n"
"                    {\n"
"                        'version': ''string'',\n"
"                        'community: ''string'',\n"
"                        'username: ''string'',\n"
"                        'authprotocol: ''string'',\n"
"                        'authpassword: ''string'',\n"
"                        'privprotocol: ''string'',\n"
"                        'privpassword: ''string'',\n"
"                        'timeout: ''integer'',\n"
"                    },\n"
"                    ...\n"
"                ]\n"
"            },\n"
"            ...\n"
"        ]\n"
"    }\n"
msgstr ""

#. type: Title ###
#, no-wrap
msgid "Example"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    {\n"
"        'id': 'job1'\n"
"        'type': 'netdiscovery',\n"
"        'workers': 2,\n"
"        'targets': [\n"
"            {\n"
"                'network': '192.168.0.0/24',\n"
"                'workers': 10,\n"
"                'timeout': 5,\n"
"                'credentials': [\n"
"                    {\n"
"                        'version': '2c',\n"
"                        'community': 'public'\n"
"                    },\n"
"                    {\n"
"                        'version': '1',\n"
"                        'community': 'public'\n"
"                    },\n"
"                ],\n"
"            },\n"
"            {\n"
"                'network': '192.168.1.1-192.168.1.254',\n"
"                'workers': 5,\n"
"                'timeout': 2,\n"
"                'credentials': [\n"
"                    {\n"
"                        'version': '3',\n"
"                        'username': 'foo',\n"
"                        'authprotocol': 'sha1',\n"
"                        'authpassword': 'bar',\n"
"                    },\n"
"                ],\n"
"            },\n"
"        ],\n"
"    },\n"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "Agent to server message"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT REQUEST (CONTENT+, DEVICEID, QUERY)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "      <!ELEMENT CONTENT (DEVICE*, MODULEVERSION, PROCESSNUMBER)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"        <!-- a device -->\n"
"        <!ELEMENT DEVICE (IP, MAC?, AUTHSNMP?, TYPE?, MANUFACTURER?, MODEL?, DESCRIPTION?, SNMPHOSTNAME?, LOCATION?, CONTACT?, SERIAL?, FIRMWARE?, UPTIME?, IPS? DNSHOSTNAME?, NETPORTVENDOR?, NETBIOSNAME?, WORKGROUP?, USERSESSION?)>\n"
"          <!--  IP address -->\n"
"          <!ELEMENT IP (#PCDATA)>\n"
"          <!--  mac address -->\n"
"          <!ELEMENT MAC (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"          <!-- Information retrieved from SNMP -->\n"
"          <!-- credentials ID -->\n"
"          <!ELEMENT AUTHSNMP (#PCDATA)>\n"
"          <!-- type\n"
"            (COMPUTER|NETWORKING|PRINTER|STORAGE|POWER|PHONE|VIDEO|KVM) -->\n"
"          <!ELEMENT TYPE (#PCDATA)>\n"
"          <!-- manufacturer -->\n"
"          <!ELEMENT MANUFACTURER (#PCDATA)>\n"
"          <!-- model -->\n"
"          <!ELEMENT MODEL (#PCDATA)>\n"
"          <!-- description (sysDescr) -->\n"
"          <!ELEMENT DESCRIPTION (#PCDATA)>\n"
"          <!--  host name -->\n"
"          <!ELEMENT SNMPHOSTNAME (#PCDATA)>\n"
"          <!--  location -->\n"
"          <!ELEMENT LOCATION (#PCDATA)>\n"
"          <!--  contact -->\n"
"          <!ELEMENT CONTACT (#PCDATA)>\n"
"          <!--  serial number -->\n"
"          <!ELEMENT SERIAL (#PCDATA)>\n"
"          <!--  firmware version -->\n"
"          <!ELEMENT FIRMWARE (#PCDATA)>\n"
"          <!--  uptime (\"X days, HH:MM::SS\" format) -->\n"
"          <!ELEMENT UPTIME (#PCDATA)>\n"
"          <!--  IP addresses -->\n"
"          <!ELEMENT IPS (IP+)>\n"
"            <!--  IP address -->\n"
"            <!ELEMENT IP (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"          <!-- Information retrieved from Nmap -->\n"
"          <!-- host name -->\n"
"          <!ELEMENT DNSHOSTNAME (#PCDATA)>\n"
"          <!-- vendor -->\n"
"          <!ELEMENT NETPORTVENDOR (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"          <!-- Information retrieved from NetBios -->\n"
"          <!-- host name -->\n"
"          <!ELEMENT NETBIOSNAME (#PCDATA)>\n"
"          <!-- user name -->\n"
"          <!ELEMENT USERSESSION (#PCDATA)>\n"
"          <!-- workgroup -->\n"
"          <!ELEMENT USERSESSION (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"        <!-- netsdiscovery module version(string) -->\n"
"        <!ELEMENT MODULEVERSION (#PCDATA)> # netdiscovery module version\n"
"        <!-- server process ID (integer) -->\n"
"        <!ELEMENT PROCESSNUMBER (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"      <!-- agent ID (string) -->\n"
"      <!ELEMENT DEVICEID (#PCDATA)> # agent ID (string)\n"
"      <!-- message type, ie \"NETDISCOVERY\" -->\n"
"      <!ELEMENT QUERY (#PCDATA)>\n"
msgstr ""

