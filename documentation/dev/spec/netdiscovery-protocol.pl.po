#. type: Title #
#, no-wrap
msgid "Network discovery protocol"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "Server to agent message"
msgstr ""

#. type: Title ###
#, no-wrap
msgid "Specification"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT REPLY (PROCESSNUMBER, OPTION)>\n"
"    <!ELEMENT PROCESSNUMBER (#PCDATA)>\n"
"    <!ELEMENT OPTION (NAME, PARAM, RANGEIP+, AUTHENTICATION+)>\n"
"    <!ELEMENT NAME (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT PARAM EMPTY>\n"
"    <!ATTLIST PARAM THREADS_QUERY CDATA \"1\">\n"
"    <!ATTLIST PARAM PID CDATA #REQUIRED>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT RANGEIP EMPTY>\n"
"    <!ATTLIST RANGEIP ID CDATA #REQUIRED>\n"
"    <!ATTLIST RANGEIP IPSTART CDATA #REQUIRED>\n"
"    <!ATTLIST RANGEIP IPEND CDATA #REQUIRED>\n"
"    <!ATTLIST RANGEIP ENTITY CDATA #IMPLIED>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!ELEMENT AUTHENTICATION EMPTY>\n"
"    <!ATTLIST AUTHENTICATION ID CDATA #REQUIRED>\n"
"    <!ATTLIST AUTHENTICATION VERSION CDATA #REQUIRED>\n"
"    <!ATTLIST AUTHENTICATION COMMUNITY CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION USERNAME CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION AUTHPROTOCOL CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION AUTHPASSPHRASE CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION PRIVHPROTOCOL CDATA #IMPLIED>\n"
"    <!ATTLIST AUTHENTICATION PRIVPASSPHRASE CDATA #IMPLIED>\n"
msgstr ""

#. type: Title ###
#, no-wrap
msgid "Example"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <?xml version=\"1.0\" encoding=\"UTF-8\"?>\n"
"    <REPLY>\n"
"      <PROCESSNUMBER>1280265592/024</PROCESSNUMBER>\n"
"      <OPTION>\n"
"        <NAME>NETDISCOVERY</NAME>\n"
"        <PARAM THREADS_DISCOVERY=\"10\" PID=\"1280265592/024\"/>\n"
"        <RANGEIP ID=\"1\" IPSTART=\"192.168.0.1\" IPEND=\"192.168.0.254\" ENTITY=\"15\"/>\n"
"        <AUTHENTICATION ID=\"1\" COMMUNITY=\"public\" VERSION=\"1\" USERNAME=\"\" AUTHPROTOCOL=\"\" AUTHPASSPHRASE=\"\" PRIVPROTOCOL=\"\" PRIVPASSPHRASE=\"\"/>\n"
"        <AUTHENTICATION ID=\"2\" COMMUNITY=\"public\" VERSION=\"2c\" USERNAME=\"\" AUTHPROTOCOL=\"\" AUTHPASSPHRASE=\"\" PRIVPROTOCOL=\"\" PRIVPASSPHRASE=\"\"/>\n"
"      </OPTION>\n"
"    </REPLY>\n"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "Agent to server message"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT REQUEST (CONTENT+, DEVICEID, QUERY)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    <!ELEMENT CONTENT (DEVICE*, MODULEVERSION, PROCESSNUMBER)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- a device -->\n"
"    <!ELEMENT DEVICE (AUTHSNMP?, TYPE?, MANUFACTURER?, MODEL?, DESCRIPTION?, SNMPHOSTNAME?, LOCATION?, CONTACT?, SERIAL?, FIRMWARE?, UPTIME?, MAC?, DNSHOSTNAME?, IP)>\n"
"    <!-- SNMP credentials ID -->\n"
"    <!ELEMENT AUTHSNMP (#PCDATA)>\n"
"    <!-- type, ie \"COMPUTER\", \"PRINTER\", \"NETWORKING\", etc... -->\n"
"    <!ELEMENT TYPE (#PCDATA)>\n"
"    <!-- manufacturer -->\n"
"    <!ELEMENT MANUFACTURER (#PCDATA)>\n"
"    <!-- model -->\n"
"    <!ELEMENT MODEL (#PCDATA)>\n"
"    <!-- description (SNMP sysDescr) -->\n"
"    <!ELEMENT DESCRIPTION (#PCDATA)>\n"
"    <!--  host name (SNMP sysName) -->\n"
"    <!ELEMENT SNMPHOSTNAME (#PCDATA)>\n"
"    <!--  location -->\n"
"    <!ELEMENT LOCATION (#PCDATA)>\n"
"    <!--  contact -->\n"
"    <!ELEMENT CONTACT (#PCDATA)>\n"
"    <!--  serial number -->\n"
"    <!ELEMENT SERIAL (#PCDATA)>\n"
"    <!--  firmware version -->\n"
"    <!ELEMENT FIRMWARE (#PCDATA)>\n"
"    <!--  uptime, formatted as \"X days, HH:MM::SS format\" -->\n"
"    <!ELEMENT UPTIME (#PCDATA)>\n"
"    <!--  mac address -->\n"
"    <!ELEMENT MAC (#PCDATA)>\n"
"    <!--  IP address -->\n"
"    <!ELEMENT IP (#PCDATA)>\n"
"    <!-- host name (DNS lookup) -->\n"
"    <!ELEMENT DNSHOSTNAME (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- netsdiscovery module version(string) -->\n"
"    <!ELEMENT MODULEVERSION (#PCDATA)> # netdiscovery module version\n"
"    <!-- server process ID (integer) -->\n"
"    <!ELEMENT PROCESSNUMBER (#PCDATA)>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    <!-- agent ID (string) -->\n"
"    <!ELEMENT DEVICEID (#PCDATA)> # agent ID (string)\n"
"    <!-- message type, ie \"NETDISCOVERY\" -->\n"
"    <!ELEMENT QUERY (#PCDATA)>\n"
msgstr ""

