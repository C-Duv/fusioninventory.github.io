#. type: Title #
#, no-wrap
msgid "API-REST-inventory"
msgstr ""

#. type: Title ##
#, no-wrap
msgid "setInventory: ?action=setInventory&devicetype=computer&machineid=$deviceid"
msgstr ""

#. type: Plain text
msgid "The inventory content is submitted in a POST request."
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"*devicetype: the object type, default is computer. This parameter is here "
"for future use.\n"
msgstr ""

#. type: Plain text
msgid "You can convert a XML inventory to the JSON format with this script:"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    #!/usr/bin/perl -w\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    use strict;\n"
"    use warnings;\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    use Data::Dumper;\n"
"    use XML::TreePP;\n"
"    use JSON;\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    use XML::TreePP;\n"
"    my $tpp = XML::TreePP->new();\n"
"    open XML, \"</tmp/myxmlinventory.ocs\" or die;\n"
"    $/=undef;\n"
"    my $xml = <XML>;\n"
"    close XML;\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    $xml =~ s/(<)([^>]*)(>)/$1.lc($2).$3/ge;\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    my $tree = $tpp->parse($xml);\n"
"    print Dumper($tree->{request}{content});\n"
msgstr ""

