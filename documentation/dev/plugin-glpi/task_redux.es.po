"    #Tuesday(1), between 0:00 and 4:00\n"
"    12345 |  1 |  0 |  4\n"
"    #Tuesday(1), between 6:00 and 10:00\n"
"    12345 |  1 |  6 | 10\n"
"    #Tuesday(1), between 12:00 and 16:00\n"
"    12345 |  1 | 12 | 16\n"
"    #Tuesday(1), between 19:00 and 0:00\n"
"    12345 |  1 | 19 | 23\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"    #Wednesday(2), between 12:00 and 15:00\n"
"    12345 |  2 | 12 | 15\n"
msgstr ""

#. type: Plain text
